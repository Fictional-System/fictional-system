{
    "version": 1,
    "default": {
        "from": [],
        "tags": {
            "latest": {
                "arguments": {
                    "FROM_TAG": "lts"
                }
            }
        },
        "arguments": [],
        "volumes": [
            "$PWD:/app:z"
        ],
        "ports": [],
        "interactive": true,
        "detached": false,
        "match_ids": true,
        "workdir": "/app"
    },
    "commands": {
        "npm": {
            "command": "npm",
            "enabled": false
        },
        "yarn": {
            "command": "yarn",
            "enabled": false
        },
        "node": {
            "command": "node",
            "enabled": false
        }
    }
}
